{
  "name": "find-duplicates",
  "version": "0.0.1",
  "description": "Find duplicates in folder",
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/revazi/js-duplicates.git"
  },
  "author": {
    "name": "Revaz Zakalashvili",
    "email": "revazzakalashvili@gmail.com",
    "url": "https://www.github.com/revazi"
  },
  "bugs": {
    "url": "https://github.com/revazi/js-duplicates/issues"
  },
  "homepage": "https://github.com/revazi/js-duplicates#readme",
  "bin": {
    "find-duplicates": "./node_modules/ts-node/dist/bin.js cli.ts"
  },
  "files": [
    "cli.ts"
  ],
  "keywords": [
    "app",
    "cli-app",
    "cli",
    "files-system",
    "duplicates",
    "find"
  ],
  "devDependencies": {
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "ava": "^3.13.0",
    "eslint": "^7.13.0",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
